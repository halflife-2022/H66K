# Software
CONFIG_PACKAGE_automount=n
CONFIG_PACKAGE_autosamba=n
CONFIG_PACKAGE_ipv6helper=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_openssh-sftp-server=y

# Applications
# 访问时间控制
CONFIG_PACKAGE_luci-app-accesscontrol=n
# 广告屏蔽大师Plus+
CONFIG_PACKAGE_luci-app-adbyby-plus=n
# IP/MAC绑定
CONFIG_PACKAGE_luci-app-arpbind=y
# 支持计划重启
CONFIG_PACKAGE_luci-app-autoreboot=n
# CPU性能优化调节
CONFIG_PACKAGE_luci-app-cpufreq=n
# DDNS
CONFIG_PACKAGE_luci-app-ddns=n
# DDNS-GO
CONFIG_PACKAGE_luci-app-ddns-go=n
# Design主题设置
CONFIG_PACKAGE_luci-app-design-config=y
# 磁盘管理工具
CONFIG_PACKAGE_luci-app-diskman=n
# 文件传输（可web安装ipk包）
CONFIG_PACKAGE_luci-app-filetransfer=n
# 添加防火墙
CONFIG_PACKAGE_luci-app-firewall=y
# VPN服务器 IPSec
CONFIG_PACKAGE_luci-app-ipsec-vpnd=n
# LXC
CONFIG_PACKAGE_luci-app-lxc=n
# 实时监控
CONFIG_PACKAGE_luci-app-netdata=y
# 网络带宽监视器
CONFIG_PACKAGE_luci-app-nlbwmon=y
# nps内网穿透
CONFIG_PACKAGE_luci-app-nps=n
# Openclash
CONFIG_PACKAGE_luci-app-openclash=n
# Passwall
CONFIG_PACKAGE_luci-app-passwall=n
# 释放内存
CONFIG_PACKAGE_luci-app-ramfree=n
# Smartdns
CONFIG_PACKAGE_luci-app-smartdns=n
# Smartdns
CONFIG_PACKAGE_luci-app-ssr-smartdns=y
# SSR科学上网Plus+
CONFIG_PACKAGE_luci-app-ssr-plus=y
# 网页终端命令行
CONFIG_PACKAGE_luci-app-ttyd=n
# Turbo ACC 网络加速
CONFIG_PACKAGE_luci-app-turboacc=y
# 解锁网易云灰色歌曲
CONFIG_PACKAGE_luci-app-unblockmusic=n
# 通用即插即用UPnP（端口自动转发）
CONFIG_PACKAGE_luci-app-upnp=y
# V2ray服务器
CONFIG_PACKAGE_luci-app-v2ray-server=y
# KMS服务器设置
CONFIG_PACKAGE_luci-app-vlmcsd=y
# FTP服务器
CONFIG_PACKAGE_luci-app-vsftpd=n
# watchcat
CONFIG_PACKAGE_luci-app-watchcat=y
# WireGuard状态
CONFIG_PACKAGE_luci-app-wireguard=n
# 流量监控
CONFIG_PACKAGE_luci-app-wrtbwmon=y
# WOL网络唤醒
CONFIG_PACKAGE_luci-app-wol=y
# 迅雷快鸟
CONFIG_PACKAGE_luci-app-xlnetacc=n
# ZeroTier内网穿透
CONFIG_PACKAGE_luci-app-zerotier=n

# Themes
# CONFIG_PACKAGE_luci-theme-argon is not set
# CONFIG_PACKAGE_luci-theme-bootstrap is not set
CONFIG_PACKAGE_luci-theme-design=y


